@model List<Employee>

    @* @model EmployeesOrderVM *@

<h1>@ViewBag.PageTitle</h1>
<hr />
<h6>@ViewBag.PageSubtitle</h6>
<table class=" table table-hover table-striped"<table>
    <thead>
        <tr>
            <th>Employee ID </th>
            <th>Employee Name</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Employee emp in Model)
            {
                <tr>
                    <td>@emp.EmployeeID</td>
                    <td>@emp.Name</td>
                    <td>@emp.City</td>
                </tr>
            }
        </tbody>
</table>
<br />
<table class=" table table-hover table-striped"<table>
    <thead>
        <tr>
            <th>Order ID </th>
            <th>Quantity</th>
            <th>Order Date</th>
            <th>Required Date</th>

        </tr>
    </thead>
    <tbody>
                @foreach (Order ord in ViewBag.Order)
                {
                    <tr>
                        <td>@ord.OrderID</td>
                        <td>@ord.Quantity</td>
                        <td>@ord.OrderDate</td>
                        <td>@ord.RequiredDate</td>
                    </tr>
                }
        </tbody>
</table>
